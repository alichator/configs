<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
    "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.user.copydirectory</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Users/alishahc/copy_directory.sh</string>
    </array>

    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>13</integer>
        <key>Minute</key>
        <integer>0</integer>
    </dict>

    <key>StandardOutPath</key>
    <string>/tmp/copydirectory.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/copydirectory.err</string>
</dict>
</plist>

